{
  "name": "default-website-template",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build --turbo",
    "dev": "next dev --turbo",
    "start": "next start",
    "format": "npx prettier src/ --write",
    "adb": "adb devices && adb reverse tcp:3000 tcp:3000",
    "db:types": "dotenv -e .env -- payload generate:types",
    "db:schema": "dotenv -e .env -- payload generate:db-schema",
    "db:importmap": "dotenv -e .env -- payload generate:importmap",
    "db": "npm run db:create && npm run db:migrate && npm run db:types && npm run db:schema && npm run db:importmap",
    "db:create": "dotenv -e .env.production -- payload migrate:create --skip-empty",
    "db:migrate": "dotenv -e .env.production -- payload migrate",
    "lint": "next lint",
    "ci": "db:migrate && npm run build",
    "payload": "payload",
    "setup": "node -e \"process.env.VERCEL_ENV || process.env.CI ? console.log('Skipping setup in CI environment') : require('child_process').execSync('node src/lib/scripts/setup.js', {stdio: 'inherit'})\"",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@next/third-parties": "^15.5.6",
    "@opennextjs/cloudflare": "^1.11.0",
    "@payloadcms/db-postgres": "^3.62.0",
    "@payloadcms/live-preview-react": "^3.62.0",
    "@payloadcms/next": "^3.62.0",
    "@payloadcms/payload-cloud": "^3.62.0",
    "@payloadcms/plugin-redirects": "^3.62.0",
    "@payloadcms/plugin-seo": "^3.62.0",
    "@payloadcms/richtext-lexical": "^3.62.0",
    "@payloadcms/storage-s3": "^3.62.0",
    "@payloadcms/ui": "^3.62.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@sentry/nextjs": "^10.22.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "dotenv-cli": "^8.0.0",
    "embla-carousel-auto-scroll": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.24",
    "geist": "^1.5.1",
    "graphql": "^16.11.0",
    "lucide-react": "^0.516.0",
    "mailersend": "^2.6.0",
    "motion": "^12.23.24",
    "next": "^15.5.6",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.10",
    "npm-check-updates": "^18.3.1",
    "payload": "^3.62.0",
    "payload-oapi": "^0.2.5",
    "posthog-js": "^1.282.0",
    "posthog-node": "^5.10.4",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.0",
    "react-countdown": "^2.3.6",
    "react-countup": "^6.5.3",
    "react-dom": "^19.2.0",
    "react-fast-marquee": "^1.6.5",
    "react-hook-form": "^7.65.0",
    "react-icons": "^5.5.0",
    "stripe": "^18.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@tailwindcss/typography": "^0.5.19",
    "@types/escape-html": "^1.0.4",
    "@types/node": "24.0.3",
    "@types/nodemailer": "^6.4.21",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "copyfiles": "^2.4.1",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.3.3",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.8.3",
    "wrangler": "^4.45.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "trustedDependencies": [
    "core-js",
    "rclone.js",
    "unrs-resolver"
  ]
}
